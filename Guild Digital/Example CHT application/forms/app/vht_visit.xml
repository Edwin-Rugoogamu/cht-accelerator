<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>VHT Visit</h:title>
    <model>
      <instance>
        <vht_visit id="vht_visit" prefix="J1!vht_visit!" delimiter="#" version="2024-01-01  19-35">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source/>
            <source_id/>
            <user>
              <contact_id/>
              <facility_id/>
            </user>
            <contact>
              <_id/>
              <name/>
              <role/>
              <phone/>
              <parent>
                <_id/>
                <parent>
                  <_id/>
                  <parent>
                    <_id/>
                  </parent>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_id/>
          <parent_1_id/>
          <parent_2_id/>
          <parent_3_id/>
          <user_phone/>
          <contact_role/>
          <created_by/>
          <created_by_person_uuid/>
          <created_by_place_uuid/>
          <act_remaining/>
          <act_type/>
          <amoxicilline_remaining/>
          <amoxicillin_type/>
          <paracetamol_remaining/>
          <zinc_ors_remaining/>
          <mrdts_remaining/>
          <coartem6_remaining/>
          <coartem12_remaining/>
          <condoms_remaining/>
          <sanitizer_remaining/>
          <gloves_remaining/>
          <masks_remaining/>
          <mama_kit_remaining/>
          <vht_visit_details>
            <note_vht_supervision/>
            <vht_visit_date/>
            <vht_available/>
            <visit-details>
              <same_phone/>
              <phone_read_only/>
              <new_phone/>
              <phone/>
              <new_captured_phone/>
              <note_vht_status/>
              <vht_completed_onboarding/>
            </visit-details>
            <g_vht_not_available>
              <vht_not_available/>
            </g_vht_not_available>
          </vht_visit_details>
          <g_device_functionality>
            <working_condition/>
            <records_synched/>
            <note_synch/>
            <device_condition/>
            <stolen_device_reported/>
            <police_report_advice/>
          </g_device_functionality>
          <stock>
            <note_1/>
            <drugs_locked_box/>
            <note_dry_place/>
            <vht_registers/>
            <referal_forms/>
            <reporting/>
            <note_2/>
            <items_repeat jr:template="">
              <item/>
              <unit/>
              <act_type/>
              <amoxicillin_type/>
              <quantity/>
              <note_4/>
            </items_repeat>
            <report_stockout>
              <stockout_supplies_drugs/>
              <report_stockout_supplies_drugs/>
              <item_expiry/>
            </report_stockout>
          </stock>
          <geolocation>
            <latitude/>
            <longitude/>
            <altitude/>
            <accuracy/>
            <gps/>
          </geolocation>
          <group_device_summary>
            <s_note_vht_visit/>
            <s_note_submit_button/>
            <s_vht_visit_value/>
            <s_device_condition/>
            <s_device_condition_1/>
            <s_device_condition_2/>
            <s_device_condition_3/>
            <s_device_condition_4/>
            <s_material_supplies/>
            <r_gloves/>
            <r_gloves_quantity/>
            <r_masks/>
            <r_masks_quantity/>
            <r_sanitizer/>
            <r_sanitizer_quantity/>
            <r_mrdts/>
            <r_mrdts_quantity/>
            <r_condoms/>
            <r_condoms_quantity/>
            <s_drugs/>
            <r_paracetamol/>
            <r_paracetamol_quantity/>
            <r_zinc_ors/>
            <r_zinc_ors_quantity/>
            <r_amoxicilline/>
            <r_amoxicilline_quantity/>
            <r_coartem6/>
            <r_coartem6_quantity/>
            <r_coartem12/>
            <r_coartem12_quantity/>
            <r_act/>
            <r_act_quantity/>
            <s_follow_up_tasks/>
            <s_device_functionality_follow/>
            <s_vht_visit_follow/>
          </group_device_summary>
          <group_device_summary_1>
            <s_note_vht_visit/>
            <s_note_submit_button/>
            <s_vht_visit_value/>
            <s_vht_not_available/>
            <s_follow_up_tasks/>
            <s_device_functionality_follow/>
            <s_vht_visit_follow/>
          </group_device_summary_1>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </vht_visit>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/vht_visit/inputs" relevant="false()"/>
      <bind nodeset="/vht_visit/inputs/source" type="string"/>
      <bind nodeset="/vht_visit/inputs/source_id" type="string"/>
      <bind nodeset="/vht_visit/inputs/user/contact_id" type="string"/>
      <bind nodeset="/vht_visit/inputs/user/facility_id" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/_id" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/name" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/role" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/phone" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/vht_visit/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/vht_visit/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/vht_visit/parent_1_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/vht_visit/parent_2_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/vht_visit/parent_3_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/vht_visit/user_phone" type="string" calculate="../inputs/contact/phone"/>
      <bind nodeset="/vht_visit/contact_role" type="string" calculate="../inputs/contact/role"/>
      <bind nodeset="/vht_visit/created_by" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/vht_visit/created_by_person_uuid" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/vht_visit/created_by_place_uuid" type="string" calculate="../inputs/user/facility_id"/>
      <bind nodeset="/vht_visit/act_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'act']/quantity, 0)"/>
      <bind nodeset="/vht_visit/act_type" type="string" calculate="/vht_visit/stock/items_repeat[item = 'act']/act_type"/>
      <bind nodeset="/vht_visit/amoxicilline_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'amoxicilline']/quantity, 0)"/>
      <bind nodeset="/vht_visit/amoxicillin_type" type="string" calculate="/vht_visit/stock/items_repeat[item = 'amoxicilline']/amoxicillin_type"/>
      <bind nodeset="/vht_visit/paracetamol_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'paracetamol']/quantity, 0)"/>
      <bind nodeset="/vht_visit/zinc_ors_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'zinc_ors']/quantity, 0)"/>
      <bind nodeset="/vht_visit/mrdts_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'mrdts']/quantity, 0)"/>
      <bind nodeset="/vht_visit/coartem6_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'coartem6']/quantity, 0)"/>
      <bind nodeset="/vht_visit/coartem12_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'coartem12']/quantity, 0)"/>
      <bind nodeset="/vht_visit/condoms_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'condoms']/quantity, 0)"/>
      <bind nodeset="/vht_visit/sanitizer_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'sanitizer']/quantity, 0)"/>
      <bind nodeset="/vht_visit/gloves_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'gloves']/quantity, 0)"/>
      <bind nodeset="/vht_visit/masks_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'masks']/quantity, 0)"/>
      <bind nodeset="/vht_visit/mama_kit_remaining" type="string" calculate="coalesce(/vht_visit/stock/items_repeat[item = 'mama_kit']/quantity, 0)"/>
      <bind nodeset="/vht_visit/vht_visit_details/note_vht_supervision" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/vht_visit_details/vht_visit_date" type="date" jr:constraintMsg="VHT visit date can't be more than 30 days ago. Also can't be a future date" required="true()" constraint=". &lt;= today() and (floor(decimal-date-time(.)) &gt;= (floor(decimal-date-time(today()) - 30)))"/>
      <bind nodeset="/vht_visit/vht_visit_details/vht_available" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details" relevant=" /vht_visit/vht_visit_details/vht_available ='yes'"/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/same_phone" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/phone_read_only" type="string" readonly="true()" relevant=" /vht_visit/vht_visit_details/visit-details/same_phone  = 'no'" calculate=" /vht_visit/user_phone "/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/new_phone" type="string" jr:constraintMsg="The phone number should be a 10 digit number and should start with either 07 or 039" relevant=" /vht_visit/vht_visit_details/visit-details/same_phone  = 'no'" required="true()" constraint="regex(.,'^(07)\d{8}$|^(039)\d{7}$')"/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/phone" type="string" readonly="true()" calculate="coalesce( /vht_visit/vht_visit_details/visit-details/new_phone ,  /vht_visit/user_phone )"/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/new_captured_phone" type="string" readonly="true()" relevant=" /vht_visit/vht_visit_details/visit-details/new_phone  != ''" calculate=" /vht_visit/vht_visit_details/visit-details/new_phone "/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/note_vht_status" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/vht_visit_details/visit-details/vht_completed_onboarding" type="select1" relevant=" /vht_visit/vht_visit_details/vht_available ='yes'" required="true()"/>
      <bind nodeset="/vht_visit/vht_visit_details/g_vht_not_available" relevant=" /vht_visit/vht_visit_details/vht_available ='no'" required="true()"/>
      <bind nodeset="/vht_visit/vht_visit_details/g_vht_not_available/vht_not_available" type="select" required="true()"/>
      <bind nodeset="/vht_visit/g_device_functionality/working_condition" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/g_device_functionality/records_synched" type="select1" relevant=" /vht_visit/g_device_functionality/working_condition  = 'yes'" required="true()"/>
      <bind nodeset="/vht_visit/g_device_functionality/note_synch" readonly="true()" type="string" relevant=" /vht_visit/g_device_functionality/records_synched  = 'no'"/>
      <bind nodeset="/vht_visit/g_device_functionality/device_condition" type="select1" relevant=" /vht_visit/g_device_functionality/working_condition  = 'no'" required="true()"/>
      <bind nodeset="/vht_visit/g_device_functionality/stolen_device_reported" type="select1" relevant=" /vht_visit/g_device_functionality/device_condition  = 'stolen'" required="true()"/>
      <bind nodeset="/vht_visit/g_device_functionality/police_report_advice" readonly="true()" type="string" relevant=" /vht_visit/g_device_functionality/stolen_device_reported  = 'no'"/>
      <bind nodeset="/vht_visit/stock/note_1" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/stock/drugs_locked_box" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/stock/note_dry_place" readonly="true()" type="string" relevant=" /vht_visit/stock/drugs_locked_box  = 'no'"/>
      <bind nodeset="/vht_visit/stock/vht_registers" type="int" required="true()"/>
      <bind nodeset="/vht_visit/stock/referal_forms" type="int" required="true()"/>
      <bind nodeset="/vht_visit/stock/reporting" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/stock/note_2" readonly="true()" type="string" relevant=" /vht_visit/stock/reporting  != ''"/>
      <bind nodeset="/vht_visit/stock/items_repeat" relevant=" /vht_visit/stock/reporting  != ''"/>
      <bind nodeset="/vht_visit/stock/items_repeat/item" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/stock/items_repeat/unit" type="string" calculate="if(selected( /vht_visit/stock/items_repeat/item , 'act'), 'blisters', if(selected( /vht_visit/stock/items_repeat/item ,'zinc_ors'),'Co-pack', if(selected( /vht_visit/stock/items_repeat/item ,'mrdts') or selected( /vht_visit/stock/items_repeat/item , 'amoxicilline') or selected( /vht_visit/stock/items_repeat/item , 'paracetamol'), 'strips', if(selected( /vht_visit/stock/items_repeat/item , 'mama_kit'),'kits', if(selected( /vht_visit/stock/items_repeat/item , 'condoms'),'boxes', 'pieces')))))"/>
      <bind nodeset="/vht_visit/stock/items_repeat/act_type" type="select1" relevant=" /vht_visit/stock/items_repeat/item  = 'act'" required="true()"/>
      <bind nodeset="/vht_visit/stock/items_repeat/amoxicillin_type" type="select1" relevant=" /vht_visit/stock/items_repeat/item  = 'amoxicilline'" required="true()"/>
      <bind nodeset="/vht_visit/stock/items_repeat/quantity" type="int" relevant=" /vht_visit/stock/items_repeat/item  != ''" required="true()" constraint=". &gt; 0"/>
      <bind nodeset="/vht_visit/stock/items_repeat/note_4" readonly="true()" type="string" relevant=" /vht_visit/stock/items_repeat/quantity  != ''"/>
      <bind nodeset="/vht_visit/stock/report_stockout/stockout_supplies_drugs" type="select1" required="true()"/>
      <bind nodeset="/vht_visit/stock/report_stockout/report_stockout_supplies_drugs" type="select" relevant=" /vht_visit/stock/report_stockout/stockout_supplies_drugs  = 'yes'" required="true()"/>
      <bind nodeset="/vht_visit/stock/report_stockout/item_expiry" type="select" required="true()"/>
      <bind nodeset="/vht_visit/geolocation/latitude" type="string" calculate="selected-at( /vht_visit/geolocation/gps , 0)"/>
      <bind nodeset="/vht_visit/geolocation/longitude" type="string" calculate="selected-at( /vht_visit/geolocation/gps , 1)"/>
      <bind nodeset="/vht_visit/geolocation/altitude" type="string" calculate="selected-at( /vht_visit/geolocation/gps , 2)"/>
      <bind nodeset="/vht_visit/geolocation/accuracy" type="string" calculate="selected-at( /vht_visit/geolocation/gps , 3)"/>
      <bind nodeset="/vht_visit/geolocation/gps" type="geopoint"/>
      <bind nodeset="/vht_visit/group_device_summary" relevant=" /vht_visit/vht_visit_details/vht_available ='yes'"/>
      <bind nodeset="/vht_visit/group_device_summary/s_note_vht_visit" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/s_note_submit_button" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/s_vht_visit_value" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/s_device_condition" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/s_device_condition_1" readonly="true()" type="string" relevant="selected( /vht_visit/g_device_functionality/device_condition , 'broken')"/>
      <bind nodeset="/vht_visit/group_device_summary/s_device_condition_2" readonly="true()" type="string" relevant="selected( /vht_visit/g_device_functionality/device_condition , 'not_charged')"/>
      <bind nodeset="/vht_visit/group_device_summary/s_device_condition_3" readonly="true()" type="string" relevant="selected( /vht_visit/g_device_functionality/device_condition , 'stolen')"/>
      <bind nodeset="/vht_visit/group_device_summary/s_device_condition_4" readonly="true()" type="string" relevant="selected( /vht_visit/g_device_functionality/device_condition , 'device_freeze')"/>
      <bind nodeset="/vht_visit/group_device_summary/s_material_supplies" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/r_gloves" readonly="true()" type="string" relevant="int( /vht_visit/gloves_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_gloves_quantity" readonly="true()" type="string" relevant="int( /vht_visit/gloves_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_masks" readonly="true()" type="string" relevant="int( /vht_visit/masks_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_masks_quantity" readonly="true()" type="string" relevant="int( /vht_visit/masks_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_sanitizer" readonly="true()" type="string" relevant="int( /vht_visit/sanitizer_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_sanitizer_quantity" readonly="true()" type="string" relevant="int( /vht_visit/sanitizer_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_mrdts" readonly="true()" type="string" relevant="int( /vht_visit/mrdts_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_mrdts_quantity" readonly="true()" type="string" relevant="int( /vht_visit/mrdts_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_condoms" readonly="true()" type="string" relevant="int( /vht_visit/condoms_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_condoms_quantity" readonly="true()" type="string" relevant="int( /vht_visit/condoms_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/s_drugs" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/r_paracetamol" readonly="true()" type="string" relevant="int( /vht_visit/paracetamol_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_paracetamol_quantity" readonly="true()" type="string" relevant="int( /vht_visit/paracetamol_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_zinc_ors" readonly="true()" type="string" relevant="int( /vht_visit/zinc_ors_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_zinc_ors_quantity" readonly="true()" type="string" relevant="int( /vht_visit/zinc_ors_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_amoxicilline" readonly="true()" type="string" relevant="int( /vht_visit/amoxicilline_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_amoxicilline_quantity" readonly="true()" type="string" relevant="int( /vht_visit/amoxicilline_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_coartem6" readonly="true()" type="string" relevant="int( /vht_visit/coartem6_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_coartem6_quantity" readonly="true()" type="string" relevant="int( /vht_visit/coartem6_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_coartem12" readonly="true()" type="string" relevant="int( /vht_visit/coartem12_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_coartem12_quantity" readonly="true()" type="string" relevant="int( /vht_visit/coartem12_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_act" readonly="true()" type="string" relevant="int( /vht_visit/act_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/r_act_quantity" readonly="true()" type="string" relevant="int( /vht_visit/act_remaining ) &gt; 0"/>
      <bind nodeset="/vht_visit/group_device_summary/s_follow_up_tasks" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/s_device_functionality_follow" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary/s_vht_visit_follow" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary_1" relevant=" /vht_visit/vht_visit_details/vht_available ='no'"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_note_vht_visit" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_note_submit_button" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_vht_visit_value" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_vht_not_available" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_follow_up_tasks" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_device_functionality_follow" readonly="true()" type="string" relevant="selected( /vht_visit/g_device_functionality/device_condition , 'stolen') and selected( /vht_visit/g_device_functionality/stolen_device_reported , 'no')"/>
      <bind nodeset="/vht_visit/group_device_summary_1/s_vht_visit_follow" readonly="true()" type="string"/>
      <bind nodeset="/vht_visit/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/vht_visit/inputs">
      <label></label>
      <group ref="/vht_visit/inputs/user">
        <label></label>
      </group>
      <group ref="/vht_visit/inputs/contact">
        <label></label>
        <input appearance="select-contact type-person" ref="/vht_visit/inputs/contact/_id">
          <label>The user being replaced</label>
        </input>
        <group appearance="hidden" ref="/vht_visit/inputs/contact/parent">
          <label></label>
          <group ref="/vht_visit/inputs/contact/parent/parent">
            <label></label>
            <group ref="/vht_visit/inputs/contact/parent/parent/parent">
              <label></label>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="Field-list" ref="/vht_visit/vht_visit_details">
      <label>VHT Availability</label>
      <input ref="/vht_visit/vht_visit_details/note_vht_supervision">
        <label>The form should be filled out when the Supervisor visits the VHT for supervisory activities</label>
      </input>
      <input ref="/vht_visit/vht_visit_details/vht_visit_date">
        <label>Date of Visit</label>
      </input>
      <select1 appearance="horizontal" ref="/vht_visit/vht_visit_details/vht_available">
        <label>Is the VHT Available at the time of the visit?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <group appearance="field-list" ref="/vht_visit/vht_visit_details/visit-details">
        <label></label>
        <select1 appearance="horizontal" ref="/vht_visit/vht_visit_details/visit-details/same_phone">
          <label>Is the VHT still using the same Contact?</label>
          <item>
            <label>Yes</label>
            <value>yes</value>
          </item>
          <item>
            <label>No</label>
            <value>no</value>
